 */
void EXTI_13_Init(void);

int main(void)
{
    /* Loop forever */
	for(;;);
}

void EXTI_13_Init(void){
	RCC->AHB1ENR |= GPIOX_CLOCK_(BUTTON) | GPIOX_CLOCK_(LED); //habilitando reloj para el pin A5 (pulsador) y pin C13 (led)

	GPIOX_MODER(MODE_DIGITAL_INPUT,BUTTON);
	//PA5 salida
	GPIOX_MODER(MODE_OUT,LED);
}







